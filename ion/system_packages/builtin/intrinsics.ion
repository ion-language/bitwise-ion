// TODO(nicolas): va_start, va_end, va_copy should be intrinsic as well

@foreign
typedef va_list = char*;

@foreign("va_start_ptr")
func va_start(args: va_list*, arg: void const*);

@intrinsic
func va_arg(args: va_list*, arg: void*);

@foreign("va_end_ptr")
func va_end(args: va_list*);

@foreign("va_copy_ptr")
func va_copy(dest: va_list*, src: va_list const*);
